 <table class="table" style="margin-top:3%" >
    <thead>
      <tr class="active" align="center">
        <th>Name</th>
       	<th>Quantity</th>
        
      </tr>
    </thead>
    <tbody>

    <c:forEach var="order" items="${itemsname}" varStatus="i">
      <tr class="active" align="center">
        <td>${order} </td>
        <td>${quantity.get(i.count-1)}</td>
        
    
      </tr>
     </c:forEach>
    </tbody>
  </table>
  <br/>

  </div>
</body>
</html>